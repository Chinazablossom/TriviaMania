<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/starter_Frg2">

    <fragment
        android:id="@+id/starter_Frg2"
        android:name="com.example.triviamania.ui.screens.StarterFrg"
        android:label="fragment_starter__frg"
        tools:layout="@layout/fragment_starter__frg" >
        <action
            android:id="@+id/action_starter_Frg2_to_categoriesFragment"
            app:destination="@id/categoriesFragment" />
        <action
            android:id="@+id/action_starter_Frg2_to_settingsFragment"
            app:destination="@id/settingsFragment" />
    </fragment>

    <fragment
        android:id="@+id/game_Fragment"
        android:name="com.example.triviamania.ui.screens.AndroidGameFragment"
        android:label="fragment_game_"
        tools:layout="@layout/fragment_androidgame_" >
        <action
            android:id="@+id/action_game_Fragment_to_resultsFragment"
            app:destination="@id/resultsFragment" />

    </fragment>


    <fragment
        android:id="@+id/categoriesFragment"
        android:name="com.example.triviamania.ui.screens.CategoriesFragment"
        android:label="fragment_categories"
        tools:layout="@layout/fragment_categories" >
        <action
            android:id="@+id/action_categoriesFragment_to_levelsFragment"
            app:destination="@id/android_levelsFragment"
            />
        <action
            android:id="@+id/action_categoriesFragment_to_scienceLevelFragment"
            app:destination="@id/scienceLevelFragment" />
        <action
            android:id="@+id/action_categoriesFragment_to_mathsFragment"
            app:destination="@id/mathsFragment" />
        <action
            android:id="@+id/action_categoriesFragment_to_computerScienceFragment"
            app:destination="@id/computerScienceFragment" />
        <action
            android:id="@+id/action_categoriesFragment_to_entertainmentFragment"
            app:destination="@id/entertainmentFragment" />
        <action
            android:id="@+id/action_categoriesFragment_to_entertainmentFragment2"
            app:destination="@id/entertainmentFragment" />
        <action
            android:id="@+id/action_categoriesFragment_to_historyFragment"
            app:destination="@id/historyFragment" />
        <action
            android:id="@+id/action_categoriesFragment_to_historyFragment2"
            app:destination="@id/historyFragment" />
    </fragment>


    <fragment
        android:id="@+id/android_levelsFragment"
        android:name="com.example.triviamania.ui.screens.AndroidLevelsFragment"
        android:label="fragment_levels"
        tools:layout="@layout/fragmentandroid_levels" >
        <action
            android:id="@+id/action_android_levelsFragment_to_game_Fragment"
            app:destination="@id/game_Fragment" />
    </fragment>
    <fragment
        android:id="@+id/resultsFragment"
        android:name="com.example.triviamania.ui.screens.ResultsFragment"
        android:label="fragment_results"
        tools:layout="@layout/fragment_results" />
    <fragment
        android:id="@+id/scienceLevelFragment"
        android:name="com.example.triviamania.ui.screens.ScienceLevelFragment"
        android:label="fragment_science_level"
        tools:layout="@layout/fragment_science_level" >
        <action
            android:id="@+id/action_scienceLevelFragment_to_scienceGameFragment"
            app:destination="@id/scienceGameFragment" />
    </fragment>
    <fragment
        android:id="@+id/scienceGameFragment"
        android:name="com.example.triviamania.ui.screens.ScienceGameFragment"
        android:label="fragment_science_game"
        tools:layout="@layout/fragment_science_game" >
        <action
            android:id="@+id/action_scienceGameFragment_to_resultsFragment"
            app:destination="@id/resultsFragment" />
    </fragment>
    <fragment
        android:id="@+id/mathsFragment"
        android:name="com.example.triviamania.ui.screens.MathsFragment"
        android:label="fragment_maths"
        tools:layout="@layout/fragment_maths_level" >
        <action
            android:id="@+id/action_mathsFragment_to_mathGameFragment"
            app:destination="@id/mathGameFragment" />
    </fragment>
    <fragment
        android:id="@+id/computerScienceFragment"
        android:name="com.example.triviamania.ui.screens.ComputerScienceFragment"
        android:label="fragment_computer_science"
        tools:layout="@layout/fragment_computer_science" >
        <action
            android:id="@+id/action_computerScienceFragment_to_comuterScienceGameFragment"
            app:destination="@id/comuterScienceGameFragment" />
    </fragment>
    <fragment
        android:id="@+id/entertainmentFragment"
        android:name="com.example.triviamania.ui.screens.EntertainmentFragment"
        android:label="fragment_entertainment"
        tools:layout="@layout/fragment_entertainment" >
        <action
            android:id="@+id/action_entertainmentFragment_to_entertainmentGameFragment"
            app:destination="@id/entertainmentGameFragment" />
    </fragment>
    <fragment
        android:id="@+id/mathGameFragment"
        android:name="com.example.triviamania.ui.screens.MathGameFragment"
        android:label="fragment_math_game"
        tools:layout="@layout/fragment_math_game" >
        <action
            android:id="@+id/action_mathGameFragment_to_resultsFragment"
            app:destination="@id/resultsFragment" />
    </fragment>
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.example.triviamania.ui.screens.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history" >
        <action
            android:id="@+id/action_historyFragment_to_historyGameFragment"
            app:destination="@id/historyGameFragment" />
    </fragment>
    <fragment
        android:id="@+id/comuterScienceGameFragment"
        android:name="com.example.triviamania.ui.screens.ComputerScienceGameFragment"
        android:label="fragment_comuter_science_game"
        tools:layout="@layout/fragment_comuter_science_game" >
        <action
            android:id="@+id/action_comuterScienceGameFragment_to_resultsFragment"
            app:destination="@id/resultsFragment" />
    </fragment>
    <fragment
        android:id="@+id/entertainmentGameFragment"
        android:name="com.example.triviamania.ui.screens.EntertainmentGameFragment"
        android:label="fragment_entertainment_game"
        tools:layout="@layout/fragment_entertainment_game" >
        <action
            android:id="@+id/action_entertainmentGameFragment_to_resultsFragment"
            app:destination="@id/resultsFragment" />
    </fragment>
    <fragment
        android:id="@+id/historyGameFragment"
        android:name="com.example.triviamania.ui.screens.HistoryGameFragment"
        android:label="fragment_history_game"
        tools:layout="@layout/fragment_history_game" >
        <action
            android:id="@+id/action_historyGameFragment_to_resultsFragment"
            app:destination="@id/resultsFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.triviamania.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" >
        <action
            android:id="@+id/action_settingsFragment_to_rulesFragment"
            app:destination="@id/rulesFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_aboutFragment"
            app:destination="@id/aboutFragment" />
    </fragment>
    <fragment
        android:id="@+id/rulesFragment"
        android:name="com.example.triviamania.ui.screens.RulesFragment"
        android:label="fragment_rules"
        tools:layout="@layout/fragment_rules" />
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.example.triviamania.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" />


</navigation>